<h2>Uploaded Products</h2>
<table>
  {{range $name, $product := .OpsMgrProducts}}
  <tr>
    <td>{{$name}}</td>
    <td>{{$product.LatestVersion}}</td>
    <td>{{$product.Marketplace}}.{{$product.MarketplaceTileName}}</td>
  </tr>
  {{end}}
</table>

<h2>Network Products</h2>
<ul>
  {{range $tile := .PivNetTiles}}
  {{if $tile.Tile}}
  <li>
    {{$tile.Slug}} - {{$tile.TileName}} - ({{$tile.TileVersion}})
  </li>
  {{end}}
  {{end}}
</ul>
